<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="4sg.xsl"?>
<root unit="%" unit-pattern="0,0">
	<title font-size="24">Munich airport departure passenger statistics 2019</title>
	<text y="70" text-color="black" font-size="9">Source: https://www.munich-airport.de/_b/0000000000000008934700bb5e9eb9ac/statistischer-jahresbericht-2019-kompr.pdf</text>
	<piles>
		<pile>
			<box id="car" color="LightskyBlue">Car</box>
			<box id="rent" color="LightskyBlue">Rental car</box>
			<box id="taxi" color="LightskyBlue">Taxi</box>
			<box id="bus" color="LightskyBlue">Bus + Transfer</box>
			<box id="train" color="LightskyBlue">Train</box>
			<box id="arr-dom" color="LightCyan">Domestic</box>
			<box id="arr-int" color="PaleTurquoise">International</box>
		</pile>
		<pile y="160">
			<box id="enter" color="LightskyBlue">Entering</box>
			<box id="switch" color="PaleTurquoise">Switching</box>
		</pile>
		<pile y="170">
			<box id="a" color="LightSlateGray">23.858.696 passengers</box>
		</pile>
		<pile y="150">
			<box id="dom" color="PaleGreen">Domestic</box>
			<box id="eur" color="MediumSeaGreen">Europe</box>
			<box id="o" color="SeaGreen">Intercontinental</box>
		</pile>
		<pile y="220">
			<box id="es" color="MediumSeaGreen">Spain</box>
			<box id="it" color="MediumSeaGreen">Italy</box>
			<box id="uk" color="MediumSeaGreen">United Kingdom</box>
			<box id="fr" color="MediumSeaGreen">France</box>
			<box id="gr" color="MediumSeaGreen">Greece</box>
			<box id="tr" color="MediumSeaGreen">Turkey</box>
			<box id="nam" color="SeaGreen" gap="60">North America</box>
			<box id="lam" color="SeaGreen">Latin America</box>
			<box id="ap" color="SeaGreen">Asia and Pacific</box>
			<box id="me" color="SeaGreen">Middle East</box>
			<box id="naf" color="SeaGreen">North Africa</box>
			<box id="oaf" color="SeaGreen">Other Africa</box>
		</pile>
	</piles>
	<flows arrow="10" color="Silver">
		<flow value="19.2" from="car" to="enter"><value/></flow>
		<flow value="5" from="rent" to="enter"><value/></flow>
		<flow value="6.8" from="taxi" to="enter"><value/></flow>
		<flow value="9.3" from="bus" to="enter"><value/></flow>
		<flow value="21.7" from="train" to="enter"><value/></flow>
		<flow value="6.5" from="arr-dom" to="switch"><value/></flow>
		<flow value="31.5" from="arr-int" to="switch"><value/></flow>
		<flow value="62" from="enter" to="a"><value/></flow>
		<flow value="38" from="switch" to="a"><value/></flow>
		<flow value="18.7" from="a" to="dom"><value/></flow>
		<flow value="57.5" from="a" to="eur"><value/></flow>
		<flow value="23.8" from="a" to="o"><value/></flow>
		<flow value="8.6" from="eur" to="es"><value/></flow>
		<flow value="7.3" from="eur" to="it"><value/></flow>
		<flow value="5.4" from="eur" to="uk"><value/></flow>
		<flow value="4.1" from="eur" to="fr"><value/></flow>
		<flow value="3.2" from="eur" to="gr"><value/></flow>
		<flow value="3.0" from="eur" to="tr"><value/></flow>
		<flow value="8.4" from="o" to="nam"><value/></flow>
		<flow value="1.7" from="o" to="lam"><value/></flow>
		<flow value="8.3" from="o" to="ap"><value/></flow>
		<flow value="2.6" from="o" to="me"><value/></flow>
		<flow value="1.7" from="o" to="naf"><value/></flow>
		<flow value="1.0" from="o" to="oaf"><value/></flow>
	</flows>
</root>
